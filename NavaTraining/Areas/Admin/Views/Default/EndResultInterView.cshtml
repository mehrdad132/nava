@model NavaTraining.Models.ResultInterView

@{
    ViewBag.Title = "EndResultInterView";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="panel panel-info">
    <div class="panel-heading">
        <i class="icon icon-2x icon-list" style="margin-left: 10px;"></i><label style="font-size: 17px;">نتیجه ی مصاحبه</label>

    </div>

    <div class="panel-body">
        <div class="table-responsive">
           
            <dl class="dl-horizontal">
                
                <dt class="h4 text-danger">
                    @Html.DisplayNameFor(model => model.ResultItem.ValueItem) :
                </dt>

                <dd class="h2 text-primary">
                    @if (Model!=null)
                    {
                        @Model.ResultItem.ValueItem
                    }
                    else
                    {
                        @TempData["msg"]
                    }
                  
                </dd>


            </dl>
            <br><br><br>
            @Html.ActionLink("بازگشت", "Index", null, new { @class = "button button-3d button-rounded button-black " })
        </div>
    </div>
</div>
